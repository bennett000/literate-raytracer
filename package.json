{
  "name": "literate-raytracer",
  "version": "0.0.0",
  "description": "a literate raytracer implementation",
  "scripts": {
    "build": "tsc --outFile ./index.js",
    "clean": "rimraf *.js && rimraf ./doc/",
    "dev": "tsc -w --outFile ./index.js",
    "predoc": "rimraf ./doc && tsc --noImplicitUseStrict --alwaysStrict false --outDir ./doc/ --sourceMap false",
    "doc": "docco -t custom.jst --css=custom.css ./doc/*.js --output=./doc/",
    "postdoc": "tsc --outFile ./doc/index.js"
  },
  "keywords": [
    "raytracer",
    "literate",
    "3d"
  ],
  "author": "",
  "license": "BSD-2-Clause",
  "devDependencies": {
    "docco": "~0.6.2",
    "rimraf": "^3.0.0",
    "typescript": "^3.7.3"
  }
}
